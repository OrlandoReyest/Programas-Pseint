Algoritmo Cociente_y_residuo_de_dos_enteros
	Definir dividendo, divisor Como enteros;
	Definir res, res1 Como Real
	Escribir "Ingresa los numeros a dividir"
	Escribir "Escribe un numero"
	Leer dividendo;
	Escribir "Escribe otro numero"
	Leer divisor;
	res<-dividendo / divisor;
	res1<-dividendo mod divisor;
	Escribir "El cocinte es ", res;
	Escribir "El residuo es ", res1;
FinAlgoritmo
