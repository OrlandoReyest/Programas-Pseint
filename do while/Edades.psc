Algoritmo Edades
	Definir edad Como entero
	definir i Como Entero
	
	Escribir "Introduzca la edad"
	leer	edad
	
	i=1;
	
	Escribir "A. Ejemplo del while"
	Mientras i<=edad
		Escribir i ". La edad es :" i
		i=i+1
	FinMientras
	Escribir "B. Ejemplo Do While"
	i=1;
	Repetir
		Escribir i ". la edad es :" i
		i=i+1
	Hasta Que i=11 
	Escribir "C. Ejemplo de For" 
	Para i=1 hasta edad Con Paso 1 Hacer
		Escribir i ". la edad es :" i
	FinPara
	
FinAlgoritmo
